<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.board.spring.yoony.category.CategoryDAO">
  <select id="selectCategoryList" resultType="com.board.spring.yoony.category.CategoryDTO">
    SELECT category_id as categoryId, name
    FROM category
    ORDER BY category_id ASC
  </select>
  <select id="test" resultType="com.board.spring.yoony.category.CategoryDTO">
    SELECT category_id as categoryId, name
    FROM category
    ORDER BY category_id ASC
    LIMIT 1
  </select>
  <select id="testInt" resultType="int">
    SELECT 1
  </select>
</mapper>